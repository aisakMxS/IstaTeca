<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.devolucion_libros.SlideshowFragment">

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context=".MainActivity"
                tools:layout_editor_absoluteX="-41dp"
                tools:layout_editor_absoluteY="-73dp">


                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="112dp"
                    android:layout_height="39dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="16dp"
                    android:text="Cédula del Estudiante:"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingConstraints" />

                <Button
                    android:id="@+id/btn_guardar"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="GUARDAR"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txt_observacciones"
                    tools:ignore="MissingConstraints" />

                <Button
                    android:id="@+id/btn_cedula"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Buscar"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.642"
                    app:layout_constraintStart_toEndOf="@+id/txtCedulaEstudiante"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:text="Titulo del Libro:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/combo_libro"
                    app:layout_constraintHorizontal_bias="0.222"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView14"
                    tools:ignore="MissingConstraints" />

                <Spinner
                    android:id="@+id/combo_libro"
                    android:layout_width="230dp"
                    android:layout_height="34dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="36dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txtCedulaEstudiante"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:text="Estado del Libro:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/txt_estado_l"
                    app:layout_constraintHorizontal_bias="0.307"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView5"
                    tools:ignore="MissingConstraints" />

                <EditText
                    android:id="@+id/txt_estado_l"
                    android:layout_width="230dp"
                    android:layout_height="34dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="36dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/combo_libro"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="32dp"
                    android:text="Fecha Entrega:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/txt_fecha_entrega"
                    app:layout_constraintHorizontal_bias="0.205"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView7"
                    tools:ignore="MissingConstraints" />

                <EditText
                    android:id="@+id/txt_fecha_entrega"
                    android:layout_width="230dp"
                    android:layout_height="34dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="36dp"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="textPersonName"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txt_estado_l"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="28dp"
                    android:text="Fecha Devolución:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/btn_fecha"
                    app:layout_constraintHorizontal_bias="0.307"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView10"
                    tools:ignore="MissingConstraints" />

                <Button
                    android:id="@+id/btn_fecha"
                    android:layout_width="124dp"
                    android:layout_height="36dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginEnd="132dp"
                    android:text="Cargar fecha"
                    android:textSize="14sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txt_fecha_entrega"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="Fecha Máxima:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/txt_fecha_maxima"
                    app:layout_constraintHorizontal_bias="0.195"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView6"
                    tools:ignore="MissingConstraints" />

                <EditText
                    android:id="@+id/txt_fecha_maxima"
                    android:layout_width="230dp"
                    android:layout_height="34dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="36dp"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="textPersonName"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/btn_fecha"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="110dp"
                    android:layout_height="39dp"
                    android:layout_marginTop="20dp"
                    android:text="Bibliotecario Entrega:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/txt_bibliotecario_entrega"
                    app:layout_constraintHorizontal_bias="0.228"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView11" />

                <EditText
                    android:id="@+id/txt_bibliotecario_entrega"
                    android:layout_width="230dp"
                    android:layout_height="34dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="36dp"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="textPersonName"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txt_fecha_maxima" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="111dp"
                    android:layout_height="41dp"
                    android:layout_marginTop="12dp"
                    android:text="Bibliotecario Recibe:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/txt_bibliotecario_recibe"
                    app:layout_constraintHorizontal_bias="0.235"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView16" />

                <EditText
                    android:id="@+id/txt_bibliotecario_recibe"
                    android:layout_width="230dp"
                    android:layout_height="34dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="36dp"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="textPersonName"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txt_bibliotecario_entrega" />

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="114dp"
                    android:layout_height="39dp"
                    android:layout_marginTop="16dp"
                    android:text="Documento Habilitado:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/txt_documento"
                    app:layout_constraintHorizontal_bias="0.258"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView17" />

                <EditText
                    android:id="@+id/txt_documento"
                    android:layout_width="230dp"
                    android:layout_height="34dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="36dp"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="textPersonName"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txt_bibliotecario_recibe" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="Penalización:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/radioGroup"
                    app:layout_constraintHorizontal_bias="0.17"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView25"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="100dp"
                    android:layout_height="47dp"
                    android:layout_marginTop="16dp"
                    android:text="Estado Prestamo:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/combo_estado"
                    app:layout_constraintHorizontal_bias="0.177"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView20"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="123dp"
                    android:layout_height="21dp"
                    android:layout_marginTop="32dp"
                    android:text="Observaciones:"
                    android:textSize="16sp"
                    app:layout_constraintEnd_toStartOf="@+id/txt_observacciones"
                    app:layout_constraintHorizontal_bias="0.307"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView12"
                    tools:ignore="MissingConstraints" />

                <EditText
                    android:id="@+id/txt_observacciones"
                    android:layout_width="230dp"
                    android:layout_height="34dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="32dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:minHeight="48dp"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/radioGroup"
                    tools:ignore="MissingConstraints" />

                <RadioGroup
                    android:id="@+id/radioGroup"
                    android:layout_width="234dp"
                    android:layout_height="49dp"
                    android:layout_marginTop="32dp"
                    android:layout_marginEnd="36dp"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/combo_estado"
                    tools:ignore="MissingConstraints">

                    <RadioButton
                        android:id="@+id/radio_si"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="34dp"
                        android:text="Si"
                        tools:ignore="MissingConstraints" />

                    <RadioButton
                        android:id="@+id/radio_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="No"
                        tools:ignore="MissingConstraints" />
                </RadioGroup>

                <EditText
                    android:id="@+id/txtCedulaEstudiante"
                    android:layout_width="160dp"
                    android:layout_height="46dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="16dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:minHeight="48dp"
                    android:textSize="12sp"
                    app:layout_constraintStart_toEndOf="@+id/textView14"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingConstraints" />

                <Spinner
                    android:id="@+id/combo_estado"
                    android:layout_width="230dp"
                    android:layout_height="34dp"
                    android:entries="@array/estadodevolucion"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="36dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/txt_documento" />


            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>