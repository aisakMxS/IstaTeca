<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.solicitud_libro.HomeFragment">

        <ScrollView

            android:layout_width="match_parent"
            android:layout_height="match_parent"

            tools:layout_editor_absoluteX="-16dp"
            tools:layout_editor_absoluteY="-151dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context=".MainActivity"
                tools:layout_editor_absoluteX="67dp"
                tools:layout_editor_absoluteY="90dp">
            <Button
                android:id="@+id/btn_modificar_solicitud"
                android:layout_width="65dp"
                android:layout_height="59dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="4dp"
                android:text="Button"
                app:layout_constraintStart_toEndOf="@+id/btn_guardar_solicitud"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/btn_imprimir_solicitud"
                android:layout_width="65dp"
                android:layout_height="59dp"
                android:layout_marginStart="36dp"
                android:layout_marginTop="4dp"
                android:text="Button"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/btn_guardar_solicitud"
                android:layout_width="65dp"
                android:layout_height="59dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="4dp"
                android:text="Button"
                app:layout_constraintStart_toEndOf="@+id/btn_imprimir_solicitud"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/btn_lista_solicitud"
                android:layout_width="65dp"
                android:layout_height="59dp"
                android:layout_marginTop="4dp"
                android:text="Button"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.343"
                app:layout_constraintStart_toEndOf="@+id/btn_modificar_solicitud"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/Txt_cedula_estudante_solicitud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="28dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:inputType="number"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_modificar_solicitud" />

            <EditText
                android:id="@+id/Txt_titulo_solicitud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Txt_codigo_DEWEY_solicitud" />

            <EditText
                android:id="@+id/Txt_estado_solicitud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Txt_titulo_solicitud" />

            <EditText
                android:id="@+id/Txt_documento_habilitante_solicitud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Txt_estado_solicitud" />

            <Button
                android:id="@+id/Btn_fechaEntrega_solicitud"
                android:layout_width="203dp"
                android:layout_height="46dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="40dp"
                android:text="Capturar fecha"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Txt_documento_habilitante_solicitud" />

            <Spinner
                android:id="@+id/Spiner_estadoPrestamo_solicitud"
                android:layout_width="229dp"
                android:layout_height="0dp"
                android:layout_marginTop="614dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="39dp"
                app:layout_constraintBottom_toTopOf="@+id/radioGroup2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/Txt_bibliotecarioEntrega_solicitud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Btn_fechaEntrega_solicitud" />

            <EditText
                android:id="@+id/Txt_fechaMaximaDev_solicitud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:inputType="date"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Txt_bibliotecarioEntrega_solicitud" />

            <EditText
                android:id="@+id/Txt_codigo_DEWEY_solicitud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:inputType="number"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Txt_cedula_estudante_solicitud" />

            <RadioButton
                android:id="@+id/Rb_domiciliario_solicitud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:text="Prestamo Domiciliario"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/Rb_intalaciones_solicitud"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView25"
                app:layout_constraintVertical_bias="0.518" />

            <RadioButton
                android:id="@+id/Rb_intalaciones_solicitud"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="Prestamo en la instalaciones"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Spiner_estadoPrestamo_solicitud"
                app:layout_constraintVertical_bias="0.458" />

            <RadioGroup
                android:id="@+id/radioGroup2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="33dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/Rb_intalaciones_solicitud"
                app:layout_constraintStart_toEndOf="@+id/Rb_domiciliario_solicitud"
                app:layout_constraintTop_toBottomOf="@+id/Spiner_estadoPrestamo_solicitud" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="101dp"
                android:layout_height="43dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="28dp"
                android:text="Cedula del Estudiante:"
                app:layout_constraintEnd_toStartOf="@+id/Txt_cedula_estudante_solicitud"
                app:layout_constraintHorizontal_bias="0.327"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_imprimir_solicitud" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="48dp"
                android:text="Codigo DEWEY:"
                app:layout_constraintEnd_toStartOf="@+id/Txt_codigo_DEWEY_solicitud"
                app:layout_constraintHorizontal_bias="0.27"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="32dp"
                android:text="Titulo Libro:"
                app:layout_constraintEnd_toStartOf="@+id/Txt_titulo_solicitud"
                app:layout_constraintHorizontal_bias="0.152"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="48dp"
                android:text="Estado del libro:"
                app:layout_constraintEnd_toStartOf="@+id/Txt_estado_solicitud"
                app:layout_constraintHorizontal_bias="0.288"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView15" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="96dp"
                android:layout_height="38dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="36dp"
                android:text="Docuemnto habilitante"
                app:layout_constraintEnd_toStartOf="@+id/Txt_documento_habilitante_solicitud"
                app:layout_constraintHorizontal_bias="0.22"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="76dp"
                android:layout_height="42dp"
                android:layout_marginTop="24dp"
                android:text="Fecha de entrega:"
                app:layout_constraintEnd_toStartOf="@+id/Btn_fechaEntrega_solicitud"
                app:layout_constraintHorizontal_bias="0.336"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView17" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="96dp"
                android:layout_height="34dp"
                android:layout_marginTop="32dp"
                android:text="Bibliotecario encargado"
                app:layout_constraintEnd_toStartOf="@+id/Txt_bibliotecarioEntrega_solicitud"
                app:layout_constraintHorizontal_bias="0.409"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView20" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="107dp"
                android:layout_height="41dp"
                android:layout_marginTop="24dp"
                android:text="Fecha maxima de devolucion:"
                app:layout_constraintEnd_toStartOf="@+id/Txt_fechaMaximaDev_solicitud"
                app:layout_constraintHorizontal_bias="0.49"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView21" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="105dp"
                android:layout_height="40dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="10dp"
                android:text="Estado del prestamo:"
                app:layout_constraintEnd_toStartOf="@+id/Spiner_estadoPrestamo_solicitud"
                app:layout_constraintHorizontal_bias="0.435"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView23" />

            <TextView
                android:id="@+id/textView36"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="40dp"
                android:layout_marginTop="56dp"
                android:text="Imprimir"
                app:layout_constraintEnd_toStartOf="@+id/textView37"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/btn_imprimir_solicitud" />

            <TextView
                android:id="@+id/textView37"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="56dp"
                android:layout_marginEnd="32dp"
                android:text="Guardar"
                app:layout_constraintEnd_toStartOf="@+id/textView38"
                app:layout_constraintTop_toTopOf="@+id/btn_guardar_solicitud" />

            <TextView
                android:id="@+id/textView38"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="56dp"
                android:layout_marginEnd="36dp"
                android:text="Modificar"
                app:layout_constraintEnd_toStartOf="@+id/textView39"
                app:layout_constraintTop_toTopOf="@+id/btn_modificar_solicitud" />

            <TextView
                android:id="@+id/textView39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="56dp"
                android:layout_marginEnd="64dp"
                android:text="Lista"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/btn_lista_solicitud" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>

    </layout>